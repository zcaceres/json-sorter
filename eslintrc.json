{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "script"
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
      "mocha/no-exclusive-tests": "error",
      "comma-dangle": 0,
      "no-var": 0,
      "func-names": 0,
      "import/newline-after-import": 0,
      "import/no-extraneous-dependencies": 0,
      "consistent-return": 0,
      "vars-on-top": 0,
      "prefer-arrow-callback": 0,
      "strict": [2, "global"],
      "no-tabs": 0,
      "import/no-dynamic-require": 0,
      "global-require": 0,
      "no-use-before-define": 0,
      "no-param-reassign": 0,
      "indent": 1,
      "prefer-template": 0,
      "object-shorthand": 0,
      "no-underscore-dangle": 0
  }
}
